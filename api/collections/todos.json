{
	"$schema": "https://www.google.com/search?q=http://json-schema.org/draft-07/schema%23",
	"type": "array",
    "minItems": 100,
    "maxItems": 100,
	"items": {
		"type": "object",
		"properties": {
          "id": {
            "type": "integer",
            "unique": "true",
            "faker": "datatype.number",
            "minimum": 1,
            "maximum": 10000
          },
          "title": {
            "type": "string",
            "faker": {
              "lorem.words": {
                "num": 3
              }
            }
          },
          "description": {
            "type": "string",
            "faker": "lorem.sentence"
          },
          "status": {
            "type": "string",
            "enum": [
              "PENDING",
              "COMPLETED"
            ]
          }
		},
		"required": ["id", "title", "description", "status"]
	}
}
